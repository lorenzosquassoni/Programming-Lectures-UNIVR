#creo una classe edificio:

class Edificio:
  def __init__(self, metri_quadrati, indirizzo, numero_civico, proprietario):
    self.metri_quadrati= metri_quadrati
    self.indirizzo= indirizzo
    self.numero_civico= numero_civico
    self.proprietario= proprietario

  def cambia_proprietario(self, nuovo_proprietario):  #cambia proprietario
    self.proprietario= nuovo_proprietario




  def __eq__(self, other):         #dico che due indirizzi con lo stesso indirizzo sono lo stesso
    return self.indirizzo== other.indirizzo and self.numero_civico== other.numero_civico


  def __repr__(self):  #ricordati che puoi concatenare solo le stringhe! fai casting degli int
    return self.indirizzo + ' ' + str(self.numero_civico) + ' ' + self.proprietario

